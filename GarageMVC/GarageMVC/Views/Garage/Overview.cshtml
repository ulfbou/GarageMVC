    <p>Currently parked vehicles</p>
</div>
<hr />

<div class="p-3 mb-2 bg-dark text-white">
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.VehicleTypes)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.VehicleTypes)
        </dd>
    </dl>

    <dl class="row">
